{% extends 'AdminCoreBundle:Common:form.html.twig' %}

{% block content_title %}
    {{ 'admin_'~class~'_edit'|trans({}, 'admin') }}
{% endblock content_title %}

{% block breadcrumb %}
    {% include "@AdminCore/Common/breadcrumb.html.twig" with {
        breadcrumb: {
            ("admin_"~class~"_list"): {
                "url": url('qbh_admin_'~class~'_list')
            },
            "edit": {
                "active" : true
            }
        }
    } %}
{% endblock breadcrumb %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            {{ render(url("qbh_admin_component_"~class~"component_editcomponent", {
                'id': id
            })) }}
        </div>
    </div>
{% endblock content %}
